{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "import",
    "simple-import-sort"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": 0,
    "simple-import-sort/imports": 2,
    "simple-import-sort/exports": 2
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}